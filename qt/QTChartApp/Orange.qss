/**********主界面样式**********/
QWidget {
    background: #323232;
    color: #b1b1b1;
}

/**********分组框**********/
QGroupBox {
    border: 2px solid rgb(233,142,35);
    background: rgb(57,58,60);
}
QGroupBox::title {
    subcontrol-position: top center;
    color: rgb(233,142,35);
}
QGroupBox#GroupBoxchart{
    border-bottom-color: transparent;
}
QGroupBox#GroupBoxdata{
    border-bottom-color: transparent;
}
QGroupBox#GroupBoxplot{
    border-bottom-color: transparent;
}
QGroupBox#GroupBoxsettemp{
    border: 2px solid rgb(100, 100, 100);
    border-radius: 0px;
}
QGroupBox#GroupBoxsethum{
    border: 2px solid rgb(100, 100, 100);
    border-radius: 0px;
}
QGroupBox#GroupBoxsettemp::title {
    color: #b1b1b1;
}
QGroupBox#GroupBoxsethum::title {
    subcontrol-position: top center;
    color: #b1b1b1;
}

/**********下拉列表**********/
QComboBox {
    height: 25px;
    border-radius: 4px;
    border: 1px solid rgb(100, 100, 100);
    background: rgb(72, 72, 73);
}

/**********输入框**********/
QLineEdit {
    height: 25px;
    border: 1px solid rgb(100, 100, 100);
    background: rgb(72, 72, 73);
}
QLineEdit:enabled {
    color: rgb(175, 175, 175);
}
QLineEdit:enabled:hover, QLineEdit:enabled:focus {
    color: rgb(230, 230, 230);
}
QLineEdit:!enabled {
    color: rgb(155, 155, 155);
}
QLineEdit#lineEdittemp{
    color:rgb(255, 85, 0);
    border-radius: 4px;
    height: 20px;
    border: 1px solid rgb(100, 100, 100);
    background: rgb(72, 72, 73);
}
QLineEdit#lineEdithum{
    color:rgb(255, 85, 0);
    border-radius: 4px;
    height: 20px;
    border: 1px solid rgb(100, 100, 100);
    background: rgb(72, 72, 73);
}
QLineEdit#lineEditTempUp{
    border: 2px solid rgb(233,142,35);
}
QLineEdit#lineEditTempLow{
    border: 2px solid rgb(233,142,35);
}
QLineEdit#lineEditHumUp{
    border: 2px solid rgb(233,142,35);
}
QLineEdit#lineEditHumLow{
    border: 2px solid rgb(233,142,35);
}
