/**********按钮**********/

QPushButton{
    border-radius: 4px;
    border: none;
    width: 75px;
    height: 25px;
}

QPushButton:enabled {
    background: rgb(233,142,35);
    color: rgb(57,58,60);
}
QPushButton:!enabled {
    background: rgb(97, 97, 97);
    color: rgb(177, 177, 177);
}
QPushButton:enabled:hover {
    background: rgb(255,184,79);
}
QPushButton:enabled:pressed {
    background: rgb(215,128,26);
}
